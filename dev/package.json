{
  "name": "dev-web",
  "private": true,
  "version": "0.1.3",
  "scripts": {
    "setup": "pnpm install --registry=https://registry.npmmirror.com",
    "dev": "cross-env ENV_TYPE=local vite",
    "build": "pnpm run build:prod",
    "build:dev": "vue-tsc && cross-env ENV_TYPE=dev vite build",
    "build:sit": "vue-tsc && cross-env ENV_TYPE=sit vite build",
    "build:uat": "vue-tsc && cross-env ENV_TYPE=uat vite build",
    "build:pre": "vue-tsc && cross-env ENV_TYPE=pre vite build",
    "build:prod": "vue-tsc && cross-env ENV_TYPE=live vite build",
    "preview": "vite preview"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@vtj/assets": "workspace:*",
    "@vtj/engine": "workspace:*",
    "@vtj/icons": "workspace:*",
    "@vtj/runtime": "workspace:*",
    "@vtj/ui": "workspace:*",
    "@vtj/utils": "workspace:*",
    "vue": "~3.3.4",
    "vue-router": "~4.2.4",
    "@element-plus/icons-vue": "~2.1.0",
    "element-plus": "~2.3.14"
  },
  "devDependencies": {
    "@vtj/cli": "workspace:*",
    "@vtj/serve": "workspace:*"
  },
