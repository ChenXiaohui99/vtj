{
  "name": "@vtj/icons",
  "private": false,
  "version": "0.6.13",
  "type": "module",
  "scripts": {
    "setup": "pnpm install --registry=https://registry.npmmirror.com",
    "svg2vue": "node scripts/svg.mjs",
    "icons": "node scripts/icons.mjs",
    "build": "npm run svg2vue && npm run icons && vue-tsc && vite build && pnpm run build:cdn",
    "build:cdn": "vue-tsc && cross-env CDN=true vite build"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "~2.1.0",
    "@vtj/cli": "^0.6.13",
    "cross-env": "~7.0.3",
    "vue": "~3.3.4",
    "xml2js": "~0.6.2"
  },
  "files": [
    "lib",
    "types",
    "cdn"
  ],
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "types/index.d.ts",
  "exports": {
    "./lib/style.css": "./lib/style.css",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "53b27ae2dc4309e33627a1d9354bfa12cde4263b"
}
