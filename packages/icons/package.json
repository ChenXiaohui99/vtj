    "xml2js": "~0.6.2"
  },
  "exports": {
    "./dist/style.css": "./dist/style.css",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
