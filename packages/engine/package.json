{
  "name": "@vtj/engine",
  "private": false,
  "version": "0.6.13",
  "type": "module",
  "scripts": {
    "setup": "npm install --registry=https://registry.npmmirror.com",
    "build": " npm run build:runtime && npm run build:shared && vue-tsc && vite build",
    "build:runtime": "vue-tsc && cross-env FILE=runtime vite build",
    "build:shared": "vue-tsc && cross-env FILE=shared vite build",
    "build:cdn": "vue-tsc && cross-env CDN=true vite build"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "~2.1.0",
    "@vtj/icons": "^0.6.13",
    "@vtj/ui": "^0.6.13",
    "@vtj/utils": "^0.6.13",
    "@vueuse/core": "~10.5.0",
    "element-plus": "~2.4.0",
    "mitt": "~3.0.1",
    "monaco-editor": "~0.44.0",
    "prettier": "~3.0.3",
    "zen-logger": "~1.1.4"
  },
  "devDependencies": {
    "@vtj/cli": "^0.6.13",
    "vue": "~3.3.4",
    "vue-router": "~4.2.5"
  },
  "files": [
    "lib",
    "types",
    "cdn"
  ],
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    "./lib/style.css": {
      "import": "./lib/style.css",
      "require": "./lib/style.css"
    },
    "./lib/runtime": {
      "types": "./types/runtime.d.ts",
      "import": "./lib/runtime.js",
      "require": "./lib/runtime.js"
    },
    "./runtime": {
      "types": "./types/runtime.d.ts",
      "import": "./lib/runtime.js",
      "require": "./lib/runtime.js"
    },
    "./lib/shared": {
      "types": "./types/shared.d.ts",
      "import": "./lib/shared.js",
      "require": "./lib/shared.js"
    },
    "./shared": {
      "types": "./types/shared.d.ts",
      "import": "./lib/shared.js",
      "require": "./lib/shared.js"
    },
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "53b27ae2dc4309e33627a1d9354bfa12cde4263b"
}
