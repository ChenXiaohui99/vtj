{
  "name": "@vtj/runtime",
  "private": false,
  "version": "0.5.0",
  "type": "module",
  "scripts": {
    "setup": "npm install --registry=https://registry.npmmirror.com",
    "dev": "cross-env ENV_TYPE=dev vite",
    "build": "vue-tsc && vite build",
    "build:cdn": "vue-tsc && cross-env CDN=true vite build",
    "test:unit": "vitest --environment jsdom",
    "coverage": "vitest run --coverage --environment jsdom"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@vtj/deps": "workspace:*",
    "@vtj/engine": "workspace:*",
    "@vtj/ui": "workspace:*",
    "@vtj/utils": "workspace:*"
  },
  "devDependencies": {
    "@vtj/cli": "workspace:*"
  },
  "files": [
    "lib",
    "types",
    "cdn"
  ],
  "main": "main.js",
  "module": "main.js",
  "types": "main.d.ts",
  "exports": {
    ".": {
      "types": "./main.d.ts",
      "import": "./main.js",
      "require": "./main.js"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
