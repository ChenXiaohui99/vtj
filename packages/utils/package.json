{
  "name": "@vtj/utils",
  "private": false,
  "version": "0.5.3",
  "type": "module",
  "scripts": {
    "setup": "pnpm install --registry=https://registry.npmmirror.com",
    "dev": "cross-env ENV_TYPE=dev vite",
    "build": "vue-tsc && vite build && pnpm run build:cdn",
