{
  "name": "vtj-core",
  "private": true,
  "devDependencies": {
    "axios": "~1.6.1",
    "fs-extra": "~11.1.1",
    "lerna": "~7.4.2",
    "jsdom": "~22.1.0",
    "@vitest/coverage-v8": "~0.34.6"
  },
  "scripts": {
    "setup": "pnpm install --registry=https://registry.npmmirror.com  && pnpm run build",
    "clean": "lerna clean -y && node scripts/clean.mjs",
    "reset": "pnpm run clean && pnpm run setup",
    "outdate": "pnpm outdated -r",
    "update": "npm config set registry=https://registry.npmmirror.com/ && pnpm up -r --latest && npm config set registry=https://registry.npmjs.org/",
    "sync": "node scripts/sync.mjs",
    "test": "lerna run --no-private test",
    "build": "lerna run --no-private build",
    "coverage": "lerna run --no-private coverage"
  },
  "engines": {
    "node": ">=20.0.0",
    "lerna": ">=7.4.0"
  },
  "packageManager": "^pnpm@8.10.3",
  "publishConfig": {
    "access": "public"
  }
}
